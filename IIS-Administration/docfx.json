{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*swagger.json"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "author": "shirhatti",
      "ms.author": "Sourabh.Shirhatti",
      "ms.topic": "conceptual",
      "searchScope": [  "IIS-Administration" ]
    },
    "fileMetadata": {
      "ms.technology": {
        "**/**.md": "iis-admin"
      },
      "ms.topic": {
        "iis-administration/api/**/**.md": "reference"
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
