{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*swagger.json"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "IIS-Administration/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "globalMetadata": {
      "breadcrumb_path": "/iis/breadcrumb/toc.json",
      "_navPath": "/foo",
      "_navRel": "/foo",
      "searchScope": ""
    },
    "fileMetadata": {
      "ms.technology": {
        "**/**.md": "iis-admin"
      },
      "ms.topic": {
        "**/**.md": "conceptual"
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
